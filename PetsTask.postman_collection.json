					"listen": "prerequest",
					"script": {
						"exec": [
							"var id = Math.floor(Math.random()*1000) +1;\r",
							"var catId = Math.floor(Math.random()*100) +1;\r",
							"var tag1Id = Math.floor(Math.random()*10) +1;\r",
							"var tag2Id = Math.floor(Math.random()*10) +1;\r",
